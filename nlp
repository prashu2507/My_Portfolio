        "# Initialize NLTK's lemmatizer\n",
        "lemmatizer = WordNetLemmatizer()\n",
        "\n",
        "# Function to preprocess text: tokenization, lemmatization, sentence segmentation\n",
        "def preprocess_text(text):\n",
        "    # Tokenize sentences using NLTK\n",
        "    sentences = sent_tokenize(text)\n",
        "    processed_sentences = []\n",
        "\n",
        "    for sentence in sentences:\n",
        "        # Tokenize words using NLTK\n",
        "        words = word_tokenize(sentence)\n",
        "\n",
        "        # Lemmatize each word using NLTK\n",
        "        lemmatized_words = [lemmatizer.lemmatize(word) for word in words]\n",
        "\n",
        "        # Recreate processed sentence\n",
        "        processed_sentence = ' '.join(lemmatized_words)\n",
        "        processed_sentences.append(processed_sentence)\n",
        "\n",
        "    return processed_sentences\n",
        "\n",
        "# Preprocess the cleaned text\n",
        "processed_text = preprocess_text(cleaned_text)\n",
