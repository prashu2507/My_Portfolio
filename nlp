        "# Initialize NLTK's lemmatizer\n",
        "lemmatizer = WordNetLemmatizer()\n",
        "\n",
        "# Function to tokenize and lemmatize sentences\n",
        "def tokenize_and_lemmatize(sentences):\n",
        "    lemmatized_sentences = []\n",
        "\n",
        "    for sentence in sentences:\n",
        "        # Tokenize words in the sentence\n",
        "        words = word_tokenize(sentence)\n",
        "\n",
        "        # Lemmatize each word\n",
        "        lemmatized_words = [lemmatizer.lemmatize(word) for word in words]\n",
        "\n",
        "        # Combine lemmatized words back into a sentence\n",
        "        lemmatized_sentence = ' '.join(lemmatized_words)\n",
        "        lemmatized_sentences.append(lemmatized_sentence)\n",
        "\n",
        "    return lemmatized_sentences\n",
        "\n",
        "# Apply tokenization and lemmatization\n",
        "lemmatized_text = tokenize_and_lemmatize(sentences)\n",
        "\n",
        "# Print the first 5 lemmatized sentences\n",
        "for i, sentence in enumerate(lemmatized_text[:5], 1):\n",
        "    print(f\"Lemmatized Sentence {i}: {sentence}\")\n"
      ],
